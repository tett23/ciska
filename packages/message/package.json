{
  "name": "@ciska/message",
  "version": "1.0.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./useCases": {
      "import": "./lib/useCases/esm/index.js",
      "module": "./lib/useCases/esm/index.js",
      "require": "./lib/useCases/cjs/index.js",
      "types": "./lib/useCases/index.d.ts"
    },
    "./client": {
      "import": "./lib/client/esm/index.js",
