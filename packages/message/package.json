      "require": "./lib/client/cjs/index.js",
      "types": "./lib/client/index.d.ts"
    }
  },
  "author": "tett23",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf lib",
    "build": "node build.mjs",
